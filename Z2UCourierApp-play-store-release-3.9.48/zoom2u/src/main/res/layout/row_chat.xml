<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="4dp"
    android:layout_marginRight="4dp"
    android:layout_marginTop="2dp"
    android:layout_marginBottom="1dp"
    android:background="@drawable/new_white_back"
    android:elevation="5dp"
    android:paddingBottom="7dp">

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/dp"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="3dp"
        android:layout_marginLeft="3dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="3dp"
        android:layout_marginRight="3dp"
        android:layout_marginBottom="3dp"
        android:src="@drawable/profile" />



    <TextView
        android:id="@+id/txtBookingRef"
        style="@style/style_15_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtPersonNameChat"
        android:layout_marginStart="5dp"
        android:layout_marginLeft="70dp"
        android:layout_marginTop="7dp"
        android:text="Booking reference" />


    <TextView
        android:id="@+id/txtDueByEta"
        style="@style/style_12_bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/txtBookingRef"
        android:layout_alignLeft="@+id/txtBookingRef"
        android:layout_marginTop="2dp"
        android:visibility="visible"
        android:text="Eta for pickup 10.15 PM" />

    <TextView
        android:id="@+id/txtDueByTime"
        style="@style/style_12_bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/txtDueByEta"
        android:layout_alignLeft="@+id/txtBookingRef"
        android:layout_marginTop="2dp"
        android:text="Due by time" />

    <TextView
        android:id="@+id/txtDropEta"
        style="@style/style_12_bold_new"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/txtDueByTime"
        android:layout_alignLeft="@+id/txtBookingRef"
        android:layout_marginTop="3dp"
        android:text="Booking chat"
        android:textColor="@color/green_bg"
        android:textSize="13dp"
        android:textStyle="bold" />

    <RelativeLayout
        android:id="@+id/txtPickUpDropOffSuburb"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_below="@+id/txtDropEta"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="5dp"
        android:orientation="horizontal"
        android:paddingStart="5dp">

        <TextView
            android:id="@+id/textPickupCompleteListB"
            style="@style/style_12_bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:fontFamily="@font/gotham_rnd_bold"
            android:text="Sydney"
            android:textSize="14dp" />

        <TextView
            android:id="@+id/textDropoffCompleteListB"
            style="@style/style_12_bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:fontFamily="@font/gotham_rnd_bold"
            android:text="Bondi junction"
            android:textSize="14dp" />


        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:layout_toStartOf="@+id/textDropoffCompleteListB"
            android:layout_toEndOf="@+id/textPickupCompleteListB"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/dot_start"
                android:layout_width="10dp"
                android:layout_height="10dp"
                android:layout_centerVertical="true"
                android:src="@drawable/dot_start" />


            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="40dp">

                <ImageView
                    android:id="@+id/dot_car"
                    android:layout_width="30dp"
                    android:layout_height="20dp"
                    android:layout_above="@id/line"
                    android:layout_centerHorizontal="true"
                    android:src="@drawable/car_new" />

                <View
                    android:id="@+id/line"
                    android:layout_width="wrap_content"
                    android:layout_height="1dp"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="10dp"
                    android:background="@color/lightgray" />

            </RelativeLayout>

            <ImageView
                android:id="@+id/dot_end"
                android:layout_width="10dp"
                android:layout_height="10dp"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:src="@drawable/dot_end" />


        </RelativeLayout>

        <TextView
            android:id="@+id/distanceBList"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textDropoffBList"
            android:layout_alignLeft="@+id/textDropoffBList"
            android:layout_marginTop="3dp"
            android:gravity="center|left"
            android:text="Distance -: 0.0 km"
            android:textColor="#374350"
            android:textSize="13dp"
            android:visibility="gone" />

    </RelativeLayout>

    <Button
        android:id="@+id/btnNotify"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_marginTop="10dp"
        android:visibility="gone"
        android:layout_marginRight="10dp"
        android:background="@drawable/circleredbgbutton"
        android:textColor="#FFFFFF" />

</RelativeLayout>

